// Complex_Numbers.cpp : This file contains the 'main' function. Program execution begins and ends there.
//

#include <iostream>
#include "Complex.hpp"
using namespace std;

int main()
{
    std::cout << "Hello World!\n";

    complexDoub CompOne(1, 2);
    complexDoub CompTwo(3, 4);
    complexDoub monk = CompTwo.conj(CompTwo);
    complexDoub ey = CompTwo.logarithm(CompTwo, 2);
    complexDoub test = CompTwo.mult(CompOne, CompOne);
    complexDoub es = CompTwo.square(CompOne);
    complexDoub chicken = CompTwo.power(CompOne, 3);
    //CompTwo.print(monk);
    CompTwo.print(test);
    cout << "\n";
    //CompTwo.print(ey);
    CompTwo.print(es);
    cout << "\n";
    CompTwo.print(chicken);
    
}
